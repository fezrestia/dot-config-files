    name = < NAME >
    email = < MAIL >

[color]
    ui = auto
    diff = auto
    status = auto
    branch = auto

[alias]
    co = checkout
    br = branch
    cm = commit
    st = status
    dif = diff
    unstage = reset HEAD --
    last = log -1 HEAD

